{
  "extends": [
    "eslint:recommended",
    "plugin:node/recommended",
    "eslint-config-airbnb-base"
  ],
  "rules": {
    "space-before-function-paren": [
      2,
      "never"
    ],
    "no-console": [
      1,
      {
        "allow": [
          "warn",
          "error"
        ]
      }
    ],
    "no-empty": [
      2,
      {
        "allowEmptyCatch": true
      }
    ],
    "no-spaced-func": 2,
    "array-bracket-spacing": [
      0,
      "always"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "quote-props": [
      2,
      "as-needed"
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "space-unary-ops": [
      2,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "comma-spacing": [
      2,
      {
        "after": true,
        "before": false
      }
    ],
    "yoda": [
      2,
      "never"
    ],
    "no-with": 2,
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "no-multiple-empty-lines": 2,
    "no-multi-str": 2,
    "one-var": [
      2,
      "never"
    ],
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "wrap-iife": 2,
    "comma-style": [
      2,
      "last"
    ],
    "space-infix-ops": 2,
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ],
    "eol-last": 2,
    "dot-notation": 2,
    "curly": [
      2,
      "all"
    ],
    "keyword-spacing": [
      2,
      {}
    ],
    "lines-around-comment": [
      1,
      {
        "beforeLineComment": true,
        "beforeBlockComment": true,
        "allowBlockStart": true,
        "allowClassStart": true
      }
    ],
    "semi": [
      2,
      "always"
    ],
    "consistent-this": [
      2,
      "_this"
    ],
    "quotes": [
      2,
      "single",
      {
        "avoidEscape": true
      }
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "max-len": [
      2,
      120
    ],
    "comma-dangle": [
      2,
      "never"
    ],
    "flowtype/require-valid-file-annotation": [
      0,
      "never"
    ],
    "no-unused-vars": [
      1,
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false
      }
    ],
    "arrow-body-style": [
      1
    ],
    "function-paren-newline": [
      0
    ],
    "arrow-parens": [
      1
    ],
    "array-callback-return": [
      1
    ],
    "no-irregular-whitespace": [
      0
    ],
    "object-curly-newline": [
      1
    ],
    "import/prefer-default-export": [
      1
    ],
    "no-plusplus": [
      1,
      {
        "allowForLoopAfterthoughts": true
      }
    ],
    "node/no-unsupported-features/es-syntax": [
      0
    ],
    "node/no-unsupported-features/node-builtins": [
      0
    ]
  }
}
